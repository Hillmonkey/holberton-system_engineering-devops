#!/usr/bin/env bash
# this script installs nginx, and installs a script with custome header
sudo -y apt-get update
sudo -y apt-get install nginx-full
sudo cd /etc/nginx
sudo rm nginx.conf
sudo wget https://raw.githubusercontent.com/Hillmonkey/holberton-system_engineering-devops/master/0x0E-web_stack_debugging_1/default
sudo service nginx restart
